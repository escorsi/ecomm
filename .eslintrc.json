{
    "env": {
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["error", "ignorePackages", { "": "never"}],
        "no-console": 0,
        "linebreak-style": 0,
        "no-unused-vars" : 0,
        "no-underscore-dangle" : 0
    }
}
